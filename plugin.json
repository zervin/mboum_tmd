{
  "name": "mboum_financial_data",
  "description": "Access MBOUM's comprehensive financial market data API",
  "parameters": {
    "type": "object",
    "properties": {
      "endpoint": {
        "type": "string",
        "enum": [
          "quotes",
          "history",
          "news",
          "modules",
          "earnings",
          "dividends",
          "options",
          "search",
          "movers"
        ],
        "description": "MBOUM API endpoint to call"
      },
      "params": {
        "type": "object",
        "description": "Parameters for the selected endpoint (e.g., {\"ticker\": \"AAPL\"} for quotes)"
      }
    },
    "required": ["endpoint"]
  }
}
